chniet/sublime-mimosa/master/packages.json": [{"last_modified": "2013-05-18 05:04:31", "description": "A MimosaJS toolset for SublimeText", "homepage": "https://github.com/blachniet/sublime-mimosa", "platforms": {"*": [{"version": "0.1.0", "url": "https://codeload.github.com/blachniet/sublime-mimosa/zipball/0.1.0"}]}, "name": "Mimosa", "author": "Brian Lachniet"}], "https://raw.github.com/ccpalettes/sublime-lorem-text/master/packages.json": [{"last_modified": "2013-02-23 01:55:53", "description": "Generate random or fixed lorem ipsum placeholder text.", "homepage": "http://ccpalettes.github.com/sublime-lorem-text", "platforms": {"*": [{"version": "1.0.3", "url": "https://codeload.github.com/ccpalettes/sublime-lorem-text/zip/v1.0.3"}]}, "name": "LoremText", "author": "Jeremy Yu"}], "https://raw.github.com/NicholasBuse/sublime_packages/master/packages.json": [{"last_modified": "2013-03-08 14:51:17", "description": "Deletes blank (or surplus blank) lines from a selection", "homepage": "https://github.com/NicholasBuse/sublime_DeleteBlankLines", "platforms": {"*": [{"version": "1.0.4", "url": "https://codeload.github.com/NicholasBuse/sublime_DeleteBlankLines/zip/v1.0.4"}]}, "name": "DeleteBlankLines", "author": "Nicholas Buse"}, {"last_modified": "2013-08-12 08:00:00", "description": "Implements an interface from ST2 to CodeAlignment for VS and Notepad++", "homepage": "http://www.codealignment.com/", "platforms": {"windows-x32": [{"version": "1.0.3", "url": "https://raw.github.com/NicholasBuse/sublime_CodeAlignment/master/v1.0.3/Code Alignment.sublime-package"}]}, "name": "Code Alignment", "author": "Nicholas Buse"}], "https://raw.github.com/superbob/SublimeTextLanguageFrench/master/packages.json": [{"last_modified": "2011-08-01 00:00:00", "description": "Spelling Language French - Dicionnaire Fran\u00e7ais", "homepage": "https://github.com/superbob/SublimeTextLanguageFrench", "platforms": {"*": [{"version": "0.1.1", "url": "https://codeload.github.com/superbob/SublimeTe